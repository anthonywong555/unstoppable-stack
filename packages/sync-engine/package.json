{
  "name": "@boilerplate/sync-engine",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist node_modules .turbo src/zero-schema.gen.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
