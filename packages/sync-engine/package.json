    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "push": "npx drizzle-kit push",
    "drizzle-zero:generate": "drizzle-zero generate -f -o src/zero-schema.gen.ts"
  },
  "keywords": [],
  "author": "",
  "description": "",
  "dependencies": {
    "@boilerplate/common": "workspace:*",
    "@boilerplate/database": "workspace:*",
    "drizzle-zero": "catalog:",
    "@rocicorp/zero": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "tsx": "^4.19.4"
  }
}
