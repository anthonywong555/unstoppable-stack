{
  "name": "@boilerplate/durable-execution",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npm run build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --outDir dist",
    "build.watch": "npm run build --watch",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@boilerplate/common": "workspace:*",
    "@temporalio/common": "catalog:",
    "@temporalio/proto": "catalog:",
    "@temporalio/client": "catalog:",
    "@temporalio/interceptors-opentelemetry": "catalog:"
  },
  "devDependencies": {
    "@boilerplate/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
